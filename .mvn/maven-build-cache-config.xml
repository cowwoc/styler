<cache xmlns="http://maven.apache.org/BUILD-CACHE-CONFIG/1.2.0"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://maven.apache.org/BUILD-CACHE-CONFIG/1.2.0 https://maven.apache.org/xsd/build-cache-config-1.2.0.xsd">
	<configuration>
		<!-- Disabled due to incompatibility with JPMS test modules -->
		<enabled>false</enabled>
		<validateXml>true</validateXml>
		<local>
			<maxBuildsCached>3</maxBuildsCached>
		</local>
		<projectVersioning adjustMetaInf="true" calculateProjectVersionChecksum="true"/>
		<attachedOutputs>
			<dirNames>
				<dirName>classes</dirName>
				<dirName>test-classes</dirName>
				<dirName>maven-status</dirName>
			</dirNames>
		</attachedOutputs>
	</configuration>
	<input>
		<global>
			<glob>{*.java,*.xml,*.properties,*.mts,*.ts,*.js,*.json}</glob>
		</global>
	</input>
	<executionControl>
		<reconcile>
			<plugins>
				<plugin artifactId="maven-compiler-plugin" goal="compile">
					<reconciles>
						<reconcile propertyName="source"/>
						<reconcile propertyName="target"/>
						<reconcile propertyName="debug"/>
						<reconcile propertyName="debuglevel"/>
						<reconcile propertyName="compilerArguments"/>
					</reconciles>
					<logs>
						<log propertyName="includes"/>
						<log propertyName="excludes"/>
						<log propertyName="argLine"/>
					</logs>
				</plugin>
				<plugin artifactId="maven-jar-plugin" goal="jar">
					<reconciles>
						<reconcile propertyName="classifier"/>
					</reconciles>
				</plugin>
				<plugin artifactId="maven-install-plugin" goal="install"/>
				<plugin artifactId="duplicate-finder-maven-plugin" goal="check">
					<reconciles>
						<reconcile propertyName="skip" skipValue="true"/>
						<reconcile propertyName="quiet" skipValue="true"/>
					</reconciles>
				</plugin>
				<plugin artifactId="maven-enforcer-plugin" goal="enforce">
					<reconciles>
						<reconcile propertyName="skip" skipValue="true"/>
					</reconciles>
					<nologs>
						<nolog propertyName="redundantProperty"/>
						<nolog propertyName="redundantProperty2"/>
						<nolog propertyName="commandLineRules"/>
					</nologs>
				</plugin>
				<plugin groupId="com.github.eirslett" artifactId="frontend-maven-plugin" goal="install-node-and-pnpm">
					<reconciles>
						<reconcile propertyName="nodeVersion"/>
						<reconcile propertyName="pnpmVersion"/>
						<reconcile propertyName="installDirectory"/>
					</reconciles>
				</plugin>
			</plugins>
		</reconcile>
	</executionControl>
</cache>